<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
<appenders>
<console name="Console" target="sSYSTEM_OUT">
<patternLayout pattern="%d{HH:mm:ss.SSS}[%t] %-5level %logger{36} -%msg%n" />
</Console>
</Appenders>
<loggers>
<Root level= "debug">
    <AppenderRef ref="Console"/>
    </Root>
    </Loggers>     
    
   </Configuration>